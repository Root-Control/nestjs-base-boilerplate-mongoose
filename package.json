    "passport-local": "^1.0.0",
    "passport-twitter-token": "^1.3.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.5",
    "socket.io-client": "^2.3.0",
    "swagger-ui-express": "^4.1.4",
    "uuid": "^8.1.0",
    "ws": "^7.3.0"
