    "aws-sdk": "^2.683.0",
    "chalk": "^4.0.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "dotenv": "^8.2.0",
    "figlet": "^1.4.0",
    "inquirer": "^7.1.0",
    "joi": "^14.3.1",
    "moment": "^2.26.0",
